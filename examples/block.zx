define MyBlock {
    let user = "Unknown";

    fn construct(name) {
        this.user = name;
    }

    fn print() {
        println(this.user);
    }
}

let block = MyBlock("Martin");
block.print();

let x = block.user;

block.user = "John";

// This will print "Martin" because x is a copy of the user field's value not a reference to it
// but 'block' is a reference to the object so 'block.user' will print "John"
println(x);
